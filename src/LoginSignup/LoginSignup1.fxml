<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<StackPane fx:id="FirstPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginSignup.LoginSignup1Controller">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" styleClass="pane22">
         <right>
            <VBox alignment="CENTER" opacity="0.84" prefHeight="400.0" prefWidth="295.0" spacing="20.0" styleClass="modal-dialog-glass" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="32.0" prefWidth="75.0" text="LOGIN">
                     <VBox.margin>
                        <Insets bottom="60.0" />
                     </VBox.margin>
                     <font>
                        <Font size="22.0" />
                     </font>
                     <textFill>
                        <LinearGradient endX="1.0" endY="1.0">
                           <stops>
                              <Stop color="#1438e8" />
                              <Stop color="BLACK" offset="1.0" />
                           </stops>
                        </LinearGradient>
                     </textFill>
                  </Label>
                  <Button fx:id="GBtn" alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onMouseClicked="#PressG" prefHeight="34.0" prefWidth="181.0" styleClass="butGG">
                     <VBox.margin>
                        <Insets bottom="75.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </right>
         <center>
            <ImageView fitHeight="328.0" fitWidth="298.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@Untitled.png" />
               </image>
            </ImageView>
         </center>
      </BorderPane>
      <BorderPane fx:id="GPane" prefHeight="200.0" prefWidth="200.0" styleClass="Web" visible="false">
         <center>
            <WebView fx:id="browser" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <HBox prefHeight="32.0" prefWidth="600.0">
               <children>
                  <Button fx:id="Gcancel" mnemonicParsing="false" onMouseClicked="#Close" prefHeight="29.0" prefWidth="78.0" styleClass="butX" text="BACK">
                     <HBox.margin>
                        <Insets left="2.0" top="2.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="Gdone" mnemonicParsing="false" onMouseClicked="#Continue" prefHeight="29.0" prefWidth="78.0" styleClass="but1" text="DONE">
                     <HBox.margin>
                        <Insets left="440.0" top="2.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </top>
      </BorderPane>
   </children>
   <stylesheets>
      <URL value="@application.css" />
      <URL value="@textfield.css" />
   </stylesheets>
</StackPane>
